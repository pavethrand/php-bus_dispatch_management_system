admin   CREATE TABLE `admin` (                                   
          `id` int(11) NOT NULL auto_increment,                  
          `username` varchar(50) NOT NULL,                       
          `email` varchar(50) NOT NULL,                          
          `password` varchar(50) NOT NULL,                       
          `trn_date` datetime NOT NULL,                          
          PRIMARY KEY  (`id`)                                    
        ) ENGINE=MyISAM AUTO_INCREMENT=7 DEFAULT CHARSET=latin1  


buses   CREATE TABLE `buses` (                               
          `busno` varchar(50) NOT NULL,                      
          `busname` varchar(50) NOT NULL,                    
          `drivername` varchar(50) NOT NULL,                 
          `start_station` varchar(50) NOT NULL,              
          `end_station` varchar(50) NOT NULL,                
          `routeno` varchar(50) NOT NULL,                    
          `atiming` varchar(50) NOT NULL,                    
          `dtiming` varchar(50) NOT NULL,                    
          `bstatus` varchar(20) NOT NULL default 'On time',  
          PRIMARY KEY  (`busno`)                             
        ) ENGINE=MyISAM DEFAULT CHARSET=latin1               

dispatcher  CREATE TABLE `dispatcher` (                               
              `id` int(11) NOT NULL auto_increment,                   
              `username` varchar(50) NOT NULL,                        
              `email` varchar(50) NOT NULL,                           
              `password` varchar(50) NOT NULL,                        
              `trn_date` datetime NOT NULL,                           
              PRIMARY KEY  (`id`)                                     
            ) ENGINE=MyISAM AUTO_INCREMENT=10 DEFAULT CHARSET=latin1  




fbuses  CREATE TABLE `fbuses` (                              
          `fname` varchar(50) NOT NULL,                      
          `busno` varchar(50) NOT NULL,                      
          `busname` varchar(50) NOT NULL,                    
          `drivername` varchar(50) NOT NULL,                 
          `start_station` varchar(50) NOT NULL,              
          `end_station` varchar(50) NOT NULL,                
          `routeno` varchar(50) NOT NULL,                    
          `atiming` varchar(50) NOT NULL,                    
          `dtiming` varchar(50) NOT NULL,                    
          `bstatus` varchar(20) NOT NULL default 'On time',  
          PRIMARY KEY  (`busno`)                             
        ) ENGINE=MyISAM DEFAULT CHARSET=latin1               



